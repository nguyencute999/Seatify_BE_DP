# Example configuration file for local development
# Copy this file to application-local.yml and fill in your actual values
# application-local.yml is already in .gitignore and won't be committed

spring:
  datasource:
    url: jdbc:mysql://localhost:3306/seatifydb?useSSL=false&serverTimezone=Asia/Ho_Chi_Minh&characterEncoding=utf8
    username: your_local_db_username
    password: your_local_db_password
    driver-class-name: com.mysql.cj.jdbc.Driver
  
  mail:
    host: smtp.gmail.com
    port: 587
    username: your_email@gmail.com
    password: your_app_password
  
  security:
    oauth2:
      client:
        registration:
          google:
            client-id: your_google_client_id
            client-secret: your_google_client_secret

seatify:
  jwt:
    secret: your-strong-jwt-secret-key-here
    expiration-ms: 86400000

cloudinary:
  cloud-name: your_cloudinary_cloud_name
  api-key: your_cloudinary_api_key
  api-secret: your_cloudinary_api_secret

